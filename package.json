{
  "name": "create-react-app-electron",
  "version": "0.1.0",
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "electron": "^1.7.5",
    "foreman": "^2.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "prettier": "^1.5.3",
    "react-scripts": "1.0.11"
  },
  "homepage": "./",
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote true --trailing-comma all --print-width 80",
      "git add"
    ]
  },
  "main": "electron/starter.js",
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node electron/wait-react",
    "precommit": "lint-staged",
    "prettier": "./node_modules/.bin/prettier --write --single-quote true --trailing-comma all --print-width 80 --write 'src/**/*.js'",
    "react-start": "react-scripts start",
    "start": "nf start -p 3000",
    "test": "react-scripts test --env=jsdom",
    "upg": "yarn upgrade-interactive"
  }
}
